{
  "name": "daily-spews-backend",
  "version": "1.0.0",
  "description": "Backend API for Daily Spews site",
  "main": "src/index.js",
  "scripts": {
    "prepare": "if [ \"$NODE_ENV\" != \"production\" ]; then husky install; fi",
    "setup-dbs": "psql -f ./src/db/setup-dbs.sql",
    "seed-dev": "node ./src/db/seeds/run-seed.js",
    "seed-prod": "NODE_ENV=production npm run seed-dev",
    "start": "node src/index.js",
    "dev": "NODE_ENV=development nodemon src/index.js",
    "test": "jest",
    "test-seed": "jest seed.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/northcoders/be-nc-news.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/northcoders/be-nc-news/issues"
  },
  "homepage": "https://github.com/northcoders/be-nc-news#readme",
  "devDependencies": {
    "husky": "^8.0.3",
    "jest": "^27.5.1",
    "jest-extended": "^2.0.0",
    "jest-sorted": "^1.0.15",
    "supertest": "^7.1.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "nodemon": "^3.1.10",
    "pg": "^8.15.6",
    "pg-format": "^1.0.4"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-extended/all",
      "jest-sorted"
    ]
  }
}
